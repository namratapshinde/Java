import java.util.Scanner;
class VolumeOfCone1 
{
      
   public static void main(String args[]) 
    {   
       
	
            	double r=Double.parseDouble(args[0]);
	
            
 
            double h=Double.parseDouble(args[1]);
      
	
       
                 double  volume=(22*r*r*h)/(3*7);
 
             System.out.println("Volume Of Cone is:" +volume);
       
 
       
          
   }
}
